@model IEnumerable<ComputerShop.Data.Models.Repair>

@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <div>
        Er zijn @Model.Count() reparaties in de database
    </div>
</div>

<ul>
    @foreach(var repair in Model)
    {
        <li>@repair.Description</li>
    }
</ul>